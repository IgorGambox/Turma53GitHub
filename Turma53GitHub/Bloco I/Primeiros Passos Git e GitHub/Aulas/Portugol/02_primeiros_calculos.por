programa {
	
	funcao inicio() {

		inteiro valor1 = 10
		inteiro valor2 = 3

		escreva("\nSomando: ", valor1 + valor2)
		escreva("\nSubtraindo: ", valor1 - valor2)
		escreva("\nMultiplicando: ", valor1 * valor2)
		escreva("\nDividindo: ", valor1 / valor2)

		escreva("\nResto da divisão: ", valor1 % valor2)
		
	}
}
